    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
